{
    "gitbook": ">=3.2.3",
    "language": "en",
    "title": "FedML",
    "description": "FedML——Doc",
    "structure": {
        "readme": "README.md",
        "summary": "SUMMARY.md"
    },
    "links": {
        "gitbook": false,
        "sharing": {
            "google": false,
            "facebook": false,
            "twitter": false,
            "all": false
        }
    },
    "plugins": [
        "-sharing",
        "-lunr", "-search", "search-plus",
        "anchor-navigation-ex-toc",
        "tbfed-pagefooter",
        "copy-code-button",
        "expandable-chapters-small",
        "splitter",
        "insert-logo",
        "hide-element"
    ],
    "pluginsConfig":{
        "anchor-navigation-ex-toc" : {
            "showLevel":true,
            "multipleH1":false,
            "mode":"float",
            "float":{
                "floatIcon": "fa fa-navicon",
                "showLevelIcon": false,
                "level1Icon": "fa fa-hand-o-right",
                "level2Icon": "fa fa-hand-o-right",
                "level3Icon": "fa fa-hand-o-right"
            }
        },
        "hide-element": {
            "elements": [".gitbook-link"]
        },
        "insert-logo":{
            "url":"/image/fedml_logo_light.png",
            "style":"background:none;max-height:100px"
        },
        "tbfed-pagefooter": {
            "copyright":"FedML",
            "modify_label": "Doc Update Time:",
            "modify_format": "YYYY-MM-DD HH:mm"
        }
    }
}